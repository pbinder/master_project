{
  "root": "libs/frontpage/dto",
  "sourceRoot": "libs/frontpage/dto/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/frontpage/dto/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/frontpage/dto"],
      "options": {
        "jestConfig": "libs/frontpage/dto/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["scope:frontpage", "type:dto"]
}
